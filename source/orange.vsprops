<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="orange"
	OutputDirectory="c:\python32\lib\site-packages\orange\libs"
	IntermediateDirectory="c:\python32\lib\site-packages\orange\obj\$(ConfigurationName)\$(ProjectName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		Optimization="3"
		FavorSizeOrSpeed="1"
		AdditionalIncludeDirectories="c:\python32\include,px,ppp,..\include,..\orange,..\orange\px"
		PreprocessorDefinitions="NDEBUG;WIN32;_WINDOWS;_USRDLL;NUMPY"
		MinimalRebuild="false"
		PrecompiledHeaderFile="$(IntDir)\$(ProjectName).pch"
		ProgramDataBaseFileName="c:\python32\lib\site-packages\orange\obj\$(ConfigurationName)\$(ProjectName).pdb"
		WarningLevel="3"
		DisableSpecificWarnings="4251;4996;4275"
	/>
	<Tool
		Name="VCLinkerTool"
		LinkIncremental="2"
		AdditionalLibraryDirectories="c:\python32\libs,c:\python32\lib\site-packages\orange\libs"
		ProgramDatabaseFile="$(IntDir)\$(ProjectName)_d.pdb"
		ImportLibrary="$(TargetDir)libs\$(TargetName).lib"
		TargetMachine="1"
	/>
</VisualStudioPropertySheet>
